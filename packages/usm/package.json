{
  "name": "usm",
  "version": "1.2.0",
  "description": "Universal state module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "start": "babel-node test/index.ts --extensions '.ts' --ignore=' ' --root-mode upward",
    "clean": "rm -rf lib",
    "check-type": "tsc --noEmit",
    "quick-build": "tsc",
    "build:lib": "babel src --out-dir lib --extensions '.ts'",
    "build": "yarn quick-build && yarn build:lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unadlib/usm.git"
  },
  "author": "unadlib",
  "license": "MIT",
  "keywords": [
    "usm",
    "universal state module",
    "state module",
    "react",
    "redux",
    "mobx",
    "vue",
    "vuex",
    "angular",
    "state management",
    "model",
    "module",
    "reactjs"
  ],
  "bugs": {
    "url": "https://github.com/unadlib/usm/issues"
  },
  "homepage": "https://github.com/unadlib/usm#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@types/jest": "^23.3.13",
    "@types/node": "^10.12.20",
    "jest": "^24.1.0",
    "typescript": "^3.2.4"
  }
}
