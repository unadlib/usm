{
  "name": "usm-vuex",
  "version": "2.0.0",
  "description": "Universal state module - usm for vuex",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "start": "babel-node test/index.ts --extensions '.ts' --ignore=' ' --root-mode upward",
    "clean": "rm -rf lib",
    "check-type": "tsc --noEmit",
    "quick-build": "tsc",
    "build:lib": "babel src --out-dir lib --extensions '.ts'",
    "build": "yarn quick-build && yarn build:lib"
  },
  "author": "unadlib",
  "license": "MIT",
  "keywords": [
    "usm",
    "universal state module",
    "state module",
    "vue",
    "vuex",
    "state management",
    "model",
    "module"
  ],
  "peerDependencies": {
    "usm": "^2.0.0",
    "vue": "^2.6.6",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-typescript": "^7.1.0",
    "typescript": "^3.3.3333",
    "vue": "^2.6.6",
    "vuex": "^3.1.0",
    "usm": "^2.0.0"
  }
}
