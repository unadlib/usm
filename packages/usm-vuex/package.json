{
  "name": "usm-vuex",
  "version": "1.1.0",
  "description": "Universal state module - usm for vuex",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "start": "babel-node test/index.ts --extensions '.ts' --ignore=' ' --root-mode upward",
    "clean": "rm -rf dist",
    "build:type": "tsc -d src/index.ts --outDir dist --lib es2017,dom --target es5",
    "build:lib": "babel src --out-dir dist --extensions '.ts' --root-mode upward",
    "build": "yarn build:type && yarn build:lib"
  },
  "author": "unadlib",
  "license": "MIT",
  "dependencies": {
    "usm": "^1.1.0",
    "vue": "^2.6.6",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "typescript": "^3.3.3333"
  }
}
